  "query": "UPDATE notification_queue nq\n            SET state = 'inprogress'\n            FROM notification n\n            WHERE nq.notification_id = n.id\n              AND nq.planned_at < NOW()\n              AND nq.state = 'planned'\n            RETURNING n.id, n.username, n.title, n.content, nq.planned_at",
